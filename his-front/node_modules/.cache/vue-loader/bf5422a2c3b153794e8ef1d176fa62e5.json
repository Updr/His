alue, cb) => {\r\n      if (this.editid) cb()\r\n      const promise = this.$axios.http.get('/constantTypes/check', { params: { name: value } })\r\n      promise.then(\r\n        response => {\r\n          if (response.data.status !== 200) {\r\n            cb(new Error('该常数类别代码已存在'))\r\n          }\r\n          cb()\r\n        }\r\n      )\r\n    }\r\n    return {\r\n      form: {\r\n        code: '',\r\n        name: ''\r\n      },\r\n      rules: {\r\n        code: [\r\n          { required: true, message: '编码不能为空', trigger: 'blur' },\r\n          { min: 1, max: 100, message: '长度在 1 到 100 个字符', trigger: 'blur' },\r\n          { validator: checkCode, trigger: 'blur' }\r\n        ],\r\n        name: [\r\n          { required: true, message: '类别名不能为空', trigger: 'blur' },\r\n          { min: 1, max: 100, message: '长度在 1 到 100 个字符', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created () {\r\n    if (this.editid) {\r\n      // 通过id 读原始数据\r\n      this.$axios.get(`/constantTypes/${this.editid}`, response => {\r\n        this.form.code = response.data.code\r\n        this.form.name = response.data.name\r\n      })\r\n    }\r\n  },\r\n  methods: {\r\n    save () {\r\n      this.$refs.form.validate((valid) => {\r\n        if (!valid) return\r\n        if (this.editid) {\r\n          this.$axios.put(`/constantTypes/${this.editid}`, response => {\r\n            // 子组件要改变父组件的值\r\n            this.$emit('update:show', false)\r\n            this.$emit('getData')\r\n          }, this.form)\r\n        } else {\r\n          this.$axios.post('/constantTypes', response => {\r\n            // 子组件要改变父组件的值\r\n            this.$emit('update:show', false)\r\n            this.$emit('getData')\r\n          }, this.form)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}