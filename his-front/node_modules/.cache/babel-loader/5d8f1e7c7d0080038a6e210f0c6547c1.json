dA;AAeA,IAAA,GAfA,eAeA,EAfA,EAeA;AAAA;;AACA,WAAA,MAAA,CAAA,GAAA,wBAAA,EAAA,GAAA,YAAA;AAAA,QAAA,MAAA,CAAA,OAAA;AAAA,OAAA;AACA,KAjBA;AAkBA,IAAA,KAlBA,iBAkBA,IAlBA,EAkBA;AACA,WAAA,MAAA,CAAA,GAAA,8BAAA,IAAA,CAAA,EAAA,GAAA,YAAA;AAAA,QAAA,IAAA,CAAA,MAAA,GAAA,CAAA;AAAA,OAAA;AACA;AApBA;AA3BA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 卡片视图区域 -->\r\n    <el-card>\r\n      <el-row :gutter=\"10\">\r\n        <el-button type=\"primary\" icon=\"el-icon-circle-plus-outline\" plain @click=\"add()\">添加项目</el-button>\r\n      </el-row>\r\n      <el-table\r\n        :data=\"tableData\"\r\n        style=\"width: 100%\"\r\n        @selection-change=\"handleSelectionChange\"\r\n      >\r\n        <el-table-column type=\"selection\"></el-table-column>\r\n        <el-table-column type=\"index\"> </el-table-column>\r\n        <el-table-column label=\"项目名称\" prop=\"itemName\"></el-table-column>\r\n        <el-table-column label=\"检验费用\" prop=\"fee\"></el-table-column>\r\n        <el-table-column label=\"创建时间\" prop=\"createtime\"></el-table-column>\r\n        <el-table-column label=\"状态\">\r\n          <template slot-scope=\"scope\">\r\n            <el-tag>{{inspect_status[scope.row.status]}}</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"220px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"danger\" icon=\"el-icon-delete\" v-if=\"scope.row.status===1\" @click=\"del(scope.row.id)\">删除项目</el-button>\r\n            <el-button type=\"primary\" icon=\"el-icon-delete\" v-if=\"scope.row.status===2\"  @click=\"check(scope.row.id)\">开立检查</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-card>\r\n\r\n    <el-dialog :title=\"'添加'+title+'项目'\" :visible.sync=\"show\" :close-on-click-modal=\"false\" :show-close=\"false\" width=\"1200px\">\r\n      <Add v-if=\"show\" :title=\"title\" :show.sync=\"show\" :caseId=\"caseId\" url=\"inspect\"></Add>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"show = false;getData();\">确 定</el-button>\r\n     </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex'\r\nimport Add from './Add'\r\nexport default {\r\n  props: ['caseId'],\r\n  data () {\r\n    return {\r\n      tableData: [],\r\n      show: false,\r\n      selectedrow: [],\r\n      title: ''\r\n    }\r\n  },\r\n  components: {\r\n    Add\r\n  },\r\n  computed: {\r\n    ...mapState(['inspect_status'])\r\n  },\r\n  mounted () {\r\n    // console.log('mounted ' + this.caseId)\r\n    // this.getData()\r\n  },\r\n  watch: {\r\n    // 正确给 caseId 赋值的 方法\r\n    caseId: function (newVal, oldVal) {\r\n      this.caseId = newVal // newVal即是caseId\r\n      this.getData()\r\n    }\r\n  },\r\n  methods: {\r\n    getData () {\r\n      this.$axios.get(`/inspectApplys/list/${this.caseId}`,\r\n        response => {\r\n          this.tableData = response.data\r\n        }\r\n      )\r\n    },\r\n    add () {\r\n      this.title = '检验'\r\n      this.show = true\r\n    },\r\n    handleSelectionChange (val) {\r\n      this.selectedrow = val // [ {} ,{} ,{}    ]\r\n    },\r\n    del (id) {\r\n      this.$axios.del(`/checkApplys/${id}`, () => { this.getData() })\r\n    },\r\n    check (item) {\r\n      this.$axios.put(`/checkApplys/check/${item.id}`, () => { item.status = 3 })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n</style>\r\n"],"sourceRoot":"src/components/doctor"}]}